{
    "plugins": [
        ["@babel/plugin-transform-react-jsx"],
        ["module-resolver", {
            "root": ["."],
            "alias": {
                "react": "preact-compat",
                "react-dom": "preact-compat",
                "create-react-class": "preact-compat/lib/create-react-class",
                "react-dom-factories": "preact-compat/lib/react-dom-factories"
            }
        }]
    ],
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "browsers": [
                    ">0.25%",
                    "not ie 11",
                    "not op_mini all"
                ]
            }
        }]
    ]
}
